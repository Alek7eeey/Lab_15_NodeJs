    <input style="border-radius: 5px" type="text" name="number" id="number" value="{{number}}" onchange="Change()" pattern="[0-9]{7}" required >
    <input type="hidden" style="border-radius: 5px" name="hiddenNumber" id="hiddenNumber" value="{{number}}">
    </br>
    <input id="button" style="border-radius: 5px" type="submit" value="update">
</form>
